<Window x:Class="Itp.HidBarcodeScanner.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Itp.HidBarcodeScanner.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <CheckBox x:Name="cbEnable" DockPanel.Dock="Top" Checked="cbEnable_Checked" Unchecked="cbEnable_Unchecked" IsChecked="True" />
        <TextBox x:Name="tb" AcceptsReturn="True" />
    </DockPanel>
</Window>
